{
    "database_system": "postgres",
    "benchmark":
        {
            "type": "standard",
            "name": "tpch",
            "scale_factor": 10
        },
    "algorithms": [
      {
            "name": "microsoft",
            "parameters": {
                "max_indexes": [0, 1, 2, 4, 8, 16],
                "max_indexes_naive": 2,
                "max_index_columns": 3
            },
            "number_of_runs": 5,
            "timeout": 300
        },
        {
            "name": "dexter",
            "parameters": {
                "min_saving_percentage": [0, 10, 25, 50, 90, 100]
            },
            "number_of_runs": 5,
            "timeout": 300
        },
        {
            "name": "epic",
            "parameters": {
                "budget": [400, 800, 1600, 3200, 6400, 12800]
            },
            "number_of_runs": 5,
            "timeout": 300
        },
        {
            "name": "ibm",
            "parameters": {
                "max_index_columns": 3,
                "budget": [400, 800, 1600, 3200, 6400, 12800],
                "try_variation_seconds_limit": 1
            },
            "number_of_runs": 5,
            "timeout": 300
        },
        {
            "name": "drop_heuristic",
            "parameters": {
                "max_indexes": [1, 2, 4, 8, 16]
            },
            "number_of_runs": 5,
            "timeout": 300
        }
    ]
}
